{
    "name": "nicolasjourdan/business-logic-bundle",
    "description": "This bundle helps you to create and to implement your business logic",
    "type": "symfony-bundle",
    "homepage": "https://github.com/NicolasJourdan/business-logic-bundle",
    "keywords": ["business", "logic"],
    "license": "MIT",
    "require": {
        "php": "^7.2|^8.0",
        "symfony/framework-bundle": "~3.4|~4.0|~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.5|~9.5",
        "symfony/maker-bundle": "^1.28"
    },
    "authors": [
        {
            "name": "Nicolas Jourdan",
            "email": "nicolas.jourdan.c@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "NicolasJourdan\\BusinessLogicBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
